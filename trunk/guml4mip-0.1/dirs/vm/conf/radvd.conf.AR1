interface eth0 {
	AdvSendAdvert on;
	MinRtrAdvInterval 1;
	MaxRtrAdvInterval 3;
	AdvHomeAgentFlag off;

	AdvHmip6MapInfo    on; 
	ReAdvHmip6MapInfo  on;
	RecvHmip6MapInfo   on;

	prefix 2000:d::/64
	{
		AdvOnLink on;
		AdvAutonomous on;
		AdvRouterAddr on; 
	};
};

interface eth1 {
	AdvSendAdvert on;
	MinRtrAdvInterval 1;
	MaxRtrAdvInterval 3;
	AdvHomeAgentFlag off;

	AdvHmip6MapInfo    on; 
	ReAdvHmip6MapInfo  on;
	RecvHmip6MapInfo   on;
};
